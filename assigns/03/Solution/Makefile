########################################################################
#
# A simple Makefile for compiling Java code for CS392X1
#
########################################################################
JAVA=java
JAVAC=javac
########################################################################
MYCPS=.:./../../../libraries
########################################################################
all::
all:: Assign03_02
# all:: Assign03_03
########################################################################
Assign03_02: \
Assign03_02.class; $(JAVA) -cp $(MYCPS) Assign03_02
Assign03_02.class: \
Assign03_02.java; $(JAVAC) -cp $(MYCPS) Assign03_02.java
########################################################################
clean:: ; rm -f *~
########################################################################
cleanall:: clean
cleanall:: ; rm -f *.class

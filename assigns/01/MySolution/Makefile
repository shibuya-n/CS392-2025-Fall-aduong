########################################################################
#
# A simple Makefile for compiling Java code for CS392X1
#
########################################################################
JAVA=java
JAVAC=javac
########################################################################
MYCPS=.:./../../assets/algs4/algs4.jar
########################################################################
all::
all:: \
TestPayroll
########################################################################
TestPayroll: TestPayroll.class; $(JAVA) -cp $(MYCPS) TestPayroll
TestPayroll.class: TestPayroll.java; $(JAVAC) -cp $(MYCPS) TestPayroll.java
########################################################################
clean:: ; rm -f *~
########################################################################
cleanall:: clean
cleanall:: ; rm -f *.class
########################################################################
########################################################################
########################################################################
